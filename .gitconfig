[user]
  name = horikawa.yoshiki
  email = yhorikawa1209@gmail.com
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  grep = auto
[alias]
  st = status
  sh = show
  so = remote show origin
  ft = fetch
  up = pull --rebase
  rbm = rebase master
  ad = add
  ci = commit
  co = checkout
  new = checkout -b
  cm = checkout master

  br = branch
  ba = branch -a

  wc = whatchanged
  ls = log --stat
  lp = log -p

  add-unmerged = "!f() { git ls-files --unmerged | cut -f2 | sort -u ; }; git add `f`"
